{
    "name": "nextdeveloper/ipaas",
    "description": "NextDeveloper abstraction library for IPaaS (Integration Platform as a Service) services",
    "type": "library",
    "license": "MIT",
    "authors": [
    ],
    "require": {
        "php": ">=8.2.0",
        "league/fractal": "*",
        "monolog/monolog": "*"
    },
    "require-dev": {
        "illuminate/support": "5.5.*",
        "illuminate/database": "5.5.*",
        "illuminate/validation": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "NextDeveloper\\IPAAS\\": "src/",
            "NextDeveloper\\IPAAS\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NextDeveloper\\IPAAS\\IPaasServiceProvider"
            ]
        }
    },
    "suggest": {}
}
